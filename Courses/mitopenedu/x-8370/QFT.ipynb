    "from qiskit import *\n",
    "from qiskit.circuit.library import QFT\n",
    "from qiskit.quantum_info import Statevector\n",
    "\n",
    "Q = QFT(2)\n",
    "QC = QuantumCircuit(2)\n",
    "# QC.h(0)\n",
    "QC.x(1)\n",
    "QC.append(Q, [0,1])\n",
    "\n",
    "QC.draw()\n"
   ]
